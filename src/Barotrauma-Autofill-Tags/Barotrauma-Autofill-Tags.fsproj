        <RuntimeIdentifiers>win10-x64;ubuntu.20.04-x64</RuntimeIdentifiers>
        <RootNamespace>Barotrauma_Autofill_Tags</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="XExtensions.fs" />
        <Compile Include="Program.fs" />
